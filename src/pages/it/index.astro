---
//translations
import i18next, { t, changeLanguage } from "i18next";
import { Trans, HeadHrefLangs } from "astro-i18next/components";
import Layout from "../../layouts/Layout.astro";
import Badge from "../../components/Badge.astro";
import LinkedinIcon from "../../components/icons/LinkedinIcon.astro";
import GithubIcon from "../../components/icons/GithubIcon.astro";
import MailIcon from "../../components/icons/MailIcon.astro";
import AboutIcon from "../../components/icons/AboutIcon.astro";
import BehnanceIcon from "../../components/icons/Behnance.astro";
import StackIcon from "../../components/icons/StackIcon.astro";
import CertificateIcon from "../../components/icons/CertificateIcon.astro";
import DesignsContent from "../../components/DesignsContent";
import Carousel from "../../components/Carousel.astro";
import Certificates from "../../components/Certificates.astro";
import SocialPill from "../../components/SocialPill.astro";
import SectionsContainers from "../../components/SectionsContainers.astro";
import StackContainer from "../../components/StackContainer.astro";
import StackDseginConteiner from "../../components/StackDseginConteiner.astro";
import Studies from "../../components/Studies.astro";
import Projects from "../../components/Projects.astro";
import ProjectsIcon from "../../components/icons/ProjectsIcon.astro";
import DesignerIcon from "../../components/icons/DesignerIcon.astro";
import DesignsContentMedia from "../../components/DesignsContentMedia";
import Footer from "../../components/Footer.astro";
import Tags from "../../components/Tags.astro";


changeLanguage("it");
---

<Layout title={t("site.title")}
description={t("site.description")}>
	<main class="px-5 w-full">
		<SectionsContainers class="py-20 md:py-40 pb-24">
			<img 
			class="rounded-full w-12 h-12"
			src="data:image/jpeg;base64," alt="Julian berardinelli logo">		
			<h1 class="text-white text-2xl md:text-3xl lg:text-4xl font-bold flex  flex-row gap-x-4 pb-3 pt-3">
				{t("main.title")}
				<a href="https://www.linkedin.com/in/juli%C3%A1n-berardinelli-3813a8248" 
				target="_blank"
				rel="noopener"
		
				class="hidden sm:flex justify-center items-center">
					<Badge>{t("main.badge")}</Badge>
				</a>
			</h1>
			<a href="https://www.linkedin.com/in/juli%C3%A1n-berardinelli-3813a8248" 
			target="_blank"
			rel="noopener"
	
			class="block sm:hidden justify-center items-center pb-4">
				<Badge>{t("main.badge")}</Badge>
			<a/>
			<h2 class="text-m lg:text-xl text-wrap opacity-90 max-w-[700px]">Sviluppatore Web <span class="text-green-300 font-semibold">Full-Stack</span> Junior e <span class="font-semibold text-purple-300">Designer Grafico</span>
			<br>
			{t("main.description.from")}<img class="w-7 inline" src="/logos/arg-flag.png" alt="">{t("main.description.to")}<img class="w-6 inline" src="/logos/eur-flag.png" alt="">
			</h2>
			<nav class="flex gap-4 mt-8 flex-wrap">
				<SocialPill href="https://www.linkedin.com/in/juli%C3%A1n-berardinelli-3813a8248" target="_blank">
					<LinkedinIcon class="size-4 lg:size-6"/>
					LinkedIn</a>
				</SocialPill>
				<SocialPill href="https://github.com/JulianBerardinelli" target="_blank">
					<GithubIcon class="size-4 lg:size-6"/>
					GitHub</a>
				</SocialPill>
				<SocialPill href="mailto:julian.berardinelli@gmail.com/" target="_blank">
					<MailIcon class="size-4 lg:size-6"/>
					julian.berardinelli@gmail.com</a>
				</SocialPill>
				<SocialPill href="https://www.behance.net/julianberardi1/" target="_blank">
					<BehnanceIcon class="size-4 lg:size-6"/>
					Behnance</a>
				</SocialPill>
			</nav>
		</SectionsContainers>

		<SectionsContainers id="about" class="pb-24 pt-52">
			<h2 class="text-2xl font-semibold mb-6 flex items-center gap-x-3">
				<AboutIcon class="size-7"/>
				{t("aboutme.title")}</h2>
			<p class="opacity-80">{t("aboutme.description")}</p>
			<SectionsContainers class="mt-10">
				<h2 class="text-xl font-semibold mb-6 flex items-center gap-x-3 text-blue-200">
					{t("aboutme.studies")}</h2>
				<Studies/>
			</SectionsContainers>
		</SectionsContainers>

		<SectionsContainers class="pb-16 lg:pb-24">
			<h2 class="text-2xl font-semibold mb-6 flex items-center gap-x-3">
				<StackIcon class="size-7"/>
				{t("knowledge.title")}</h2>
			<div class="relative">
				<h1 class="absolute text-3xl lg:text-6xl font-bold opacity-20">{t("knowledge.developer")}</h1>
			</div>
			<StackContainer />
		</SectionsContainers>
		<SectionsContainers class="pb-24">
			<div class="relative">
				<h1 class="absolute text-3xl lg:text-6xl font-bold opacity-20">{t("knowledge.designer")}</h1>
			</div>
			<StackDseginConteiner class="mt-22"/>
		</SectionsContainers>
		<SectionsContainers id="certificates">
			<h2 class="text-2xl font-semibold mb-6 flex items-center gap-x-3">
				<CertificateIcon class="size-7"/>
				{t("certificates.title")}</h2>
				<Certificates/>
		</SectionsContainers>
		<SectionsContainers class="py-10">
			<h2 class="text-2xl font-semibold mb-6 flex items-center gap-x-3">
				<ProjectsIcon class="size-7"/>
				{t("projects.title")}</h2>
			<Projects />
		</SectionsContainers>
		<SectionsContainers class="py-10">
			<h2 class="text-2xl font-semibold mb-6 flex items-center gap-x-3">
				<DesignerIcon class="size-7"/>
				{t("designerProfile.title")}</h2>
				<h2 class="flex items-center gap-x-3 text-lg lg:text-2xl font-semibold mb-4 pt-2 text-blue-200">
					{t("designerProfile.subTitle-1")}</h2>
				<p class="text-sm lg:text-lg mb-4 text-pretty opacity-70">{t("designerProfile.illustration-parrafo")}
				</p>
				<Tags/>
			<DesignsContent client:visible header="Header" />
			<h2 class="flex items-center gap-x-3 text-lg lg:text-2xl font-semibold mb-4 mt-16 text-blue-200">
				{t("designerProfile.subTitle-2")}</h2>
				<p class="text-sm lg:text-lg mb-4 pb-4 text-pretty opacity-70">{t("designerProfile.photoshop-parrafo2")}
				</p>
				<Tags/>
			<DesignsContentMedia client:visible header="Header" ></DesignsContentMedia>
			<div class="pt-4">
				<SocialPill href="https://www.behance.net/julianberardi1/" target="_blank">
					<BehnanceIcon class="size-4 lg:size-6 "/>
					Behnance
				</a>
				</SocialPill>
			</div>
		</SectionsContainers>
	</main>
	<Footer/>
</Layout>